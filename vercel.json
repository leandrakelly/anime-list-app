{
    "builds": [
      {
        "src": "packages/backend/dist/index.js",
        "use": "@vercel/node"
      },
      {
        "src": "packages/frontend/package.json",
        "use": "@vercel/next"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "packages/backend/dist/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "packages/frontend/$1"
      }
    ]
  }